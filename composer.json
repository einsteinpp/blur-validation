{
  "name": "vincentkos/blur-validation",
  "description": "Easy to use server side blur validation",
  "keywords": [
    "vincent kosciuszko",
    "vincentkos",
    "blur-validation",
    "laravel",
    "validation",
    "blur"
  ],
  "homepage": "https://github.com/vincentkosciuszko/blur-validation",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Vincent Kosciuszko",
      "email": "kosciuszkovincent@outlook.fr",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Vincentkos\\BlurValidation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vincentkos\\BlurValidation\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vincentkos\\BlurValidation\\BlurValidationServiceProvider"
      ],
      "aliases": {
        "BlurValidation": "Vincentkos\\BlurValidation\\BlurValidationFacade"
      }
    }
  }
}
